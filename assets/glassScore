    var UGERROR = {
        errors: [],
        loggerLoaded: false,
    };
    var oldOnError = window.onerror;
    var loadScript = function (src, resolve) {
        UGERROR.loggerLoaded = true;
        var script = document.createElement('script');
        script.async = true;
        script.type = 'text/javascript';
        script.src = src;

        var changeState = function () {
            if (!loaded) {
                resolve()
                loaded = true;
            }
        };
        var loaded;

        script.onreadystatechange = changeState;
        script.onload = changeState;

        var node = document.getElementsByTagName('script')[0];
        node.parentNode.insertBefore(script, node);
    };

    UGERROR.loadErrorLogger = function (release) {
        var whitelistUrls = [
            new RegExp('https://musescore.com/static/public')
        ];

        if (window.location.pathname !== '/') {
            whitelistUrls.push(new RegExp(window.location.href))
        }

        !UGERROR.loggerLoaded && loadScript('https://musescore.com/static/vendor/centry/raven.3.27.0.min.js', function () {
            window.onerror = oldOnError;
            window.Raven.setUserContext({
                userId: '0',
            })
            window.Raven.config('https://23f7f9adc71e43919814a71a87ea2c52@sentry10.ultimate-guitar.com/6', {
                release: release,
                whitelistUrls: whitelistUrls,
                tags: {"country":"CA","isProUser":false,"isAuthenticated":false}            }).install();
            UGERROR.errors.forEach(function (args) {
                (args.length > 1) ? window.onerror.apply(this, args) : window.Raven.captureException(args);
            });
        })
    };

    window.onerror = function () {
        UGERROR.errors.push(arguments);
        oldOnError && oldOnError.apply(this, arguments);

        if (!UGERROR.hasFirstError) {
            function load() {
                UGERROR.loadErrorLogger("mu-web_1.08.91");
            }

            if (document.readyState === 'complete') {
                load();
            } else {
                window.addEventListener('load', load)
            }
            UGERROR.hasFirstError = true
        }
    };
</script>
